{
    "Logging": {
        "LogLevel": {
            "Default": "Information",
            "Microsoft.AspNetCore": "Warning"
        }
    },
    "AllowedHosts": "*",
    "Settings": {
        "EnableSwagger": true,
        "DocsEndpoint": "docs",
        "DocsSwaggerOptions": {
            "HeaderDescription": "ODNS API Documentation",
            "Title": "ODNS API",
            "ContactName": "TUD",
            "ContactEmail": "m.waehlisch@tu-dresden.de"
        },
        "PortLinux": 5551,
        "Caching": {
            "UseTimeout": true,
            "TimeoutMinutes": 30
        }
    },
    "Database": {
        "dbtype": "POSTSQL",
        "ConnectionString": "",
        "Functions": {
            "GetDnsEntries": "odns.get_dns_entries"
        }
    },
    "RateLimiting": {
        "PolicyName": "fixed",
        "PermitLimit": 2,
        "WindowInSeconds": 10,
        "QueueLimit": 0
    }
}
